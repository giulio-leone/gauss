{
  "name": "@onegenui/deep-agents",
  "version": "0.1.0",
  "description": "Deep Agent Framework built on Vercel AI SDK v6 â€” planning, context management, subagents, persistent memory, MCP integration",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.js",
      "require": "./dist/node/index.cjs"
    },
    "./deno": {
      "types": "./dist/deno/index.d.ts",
      "import": "./dist/deno/index.js"
    },
    "./edge": {
      "types": "./dist/edge/index.d.ts",
      "import": "./dist/edge/index.js"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "import": "./dist/browser/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "ai": "^6.0.68",
    "zod": "^4.0.0"
  },
  "peerDependencies": {
    "@ai-sdk/mcp": "^1.0.0",
    "@supabase/supabase-js": "^2.0.0",
    "tiktoken": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@supabase/supabase-js": {
      "optional": true
    },
    "tiktoken": {
      "optional": true
    },
    "@ai-sdk/mcp": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "rimraf": "^6.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}