    },
    "./workflow": {
      "types": "./dist/workflow/index.d.ts",
      "import": "./dist/workflow/index.js",
      "require": "./dist/workflow/index.cjs"
    },
    "./a2a": {
      "types": "./dist/a2a/index.d.ts",
      "import": "./dist/a2a/index.js",
      "require": "./dist/a2a/index.cjs"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.js",
      "require": "./dist/testing/index.cjs"
    },
    "./runtime/node": {
      "types": "./dist/runtime-node.d.ts",
      "import": "./dist/runtime-node.js"
    },
    "./runtime/deno": {
      "types": "./dist/runtime-deno.d.ts",
      "import": "./dist/runtime-deno.js"
    },
    "./runtime/edge": {
      "types": "./dist/runtime-edge.d.ts",
      "import": "./dist/runtime-edge.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.js",
      "require": "./dist/providers/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "gaussflow": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:quality:semantic": "vitest run src/evals/__tests__/semantic-search.quality-gate.test.ts src/evals/__tests__/semantic-search-harness.test.ts",
    "test:benchmark:semantic": "vitest run src/evals/__tests__/semantic-search-benchmark.test.ts src/evals/__tests__/semantic-search.benchmark-gate.test.ts",
    "test:stress:semantic": "vitest run src/evals/__tests__/semantic-search-stress-suite.test.ts src/evals/__tests__/semantic-search.stress-gate.test.ts",
    "quality:gate": "npm run test:quality:semantic",
    "benchmark:gate": "npm run test:benchmark:semantic",
    "stress:gate": "npm run test:stress:semantic",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "ai": "^6.0.86",
    "zod": "^3.25.76"
  },
